{
  "name": "nodejs-blog",
  "version": "0.0.3",
  "description": "A blog API built in NodeJS that can be used as a standalone application, or included in a NodeJS application",
  "main": "index.js",
  "author": "Luciano Nooijen <luciano@bytecode.nl> (https://bytecode.nl)",
  "license": "GPL-3.0-only",
  "private": false,
  "homepage": "https://github.com/lucianonooijen/NodeJS-Blog",
  "bugs": {
    "url": "https://github.com/lucianonooijen/NodeJS-Blog/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucianonooijen/NodeJS-Blog"
  },
  "keywords": [
    "blog",
    "bytecode",
    "node-blog",
    "nodejs-blog",
    "api"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "dev": "nodemon server/server.js",
    "start": "node server/server.js",
    "lint": "eslint --ext .jsx,.js,.ts .",
    "test": "NODE_ENV=test jest -i",
    "test:watch": "NODE_ENV=test jest -i --watch",
    "coverage": "NODE_ENV=test jest -i --coverage",
    "migrate": "knex migrate:latest",
    "reinstall": "rm -rf node_modules && yarn"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "helmet": "^3.14.0",
    "knex": "^0.15.2",
    "morgan": "^1.9.1",
    "pg": "^7.6.0",
    "sqreen": "^1.26.2",
    "yarn": "^1.12.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.26.2",
    "eslint-plugin-jquery": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.5",
    "prettier": "^1.14.3",
    "prettier-eslint": "^8.8.2",
    "prettierrc": "^0.0.0-5",
    "response-time": "^2.3.2",
    "sqlite3": "^4.0.3",
    "supertest": "^3.3.0"
  },
  "env": {
    "params": {
      "env": "NODE_ENV=test"
    }
